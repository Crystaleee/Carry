body {
	font-family: "微软雅黑";
}

.leaflet-container {
	background-image: url(../../static/img/bk-right-content.png);
	background-size: 100% auto;
}

.leaflet-right {
	margin-right: 20px;
}

.leaflet-control-attribution {
	display: none;
}

.infoTips {
	position: fixed;
	width: auto;
	height: auto;
	padding: 6px 8px;
	font-family: "微软雅黑";
	font-size: 14px;
	background: white;
	background: rgba(255, 255, 255, 0.8);
	box-shadow: 0 0 15px rgba(0, 0, 0, 0.2);
	border-radius: 5px;
	line-height: 30px;
}

.info {
	width: 200px;
	padding: 6px 8px;
	font-family: "微软雅黑";
	font-size: 14px;
	background: white;
	background: rgba(255, 255, 255, 0.8);
	box-shadow: 0 0 15px rgba(0, 0, 0, 0.2);
	border-radius: 5px;
}

.info h4 {
	margin: 0 0 5px;
	color: #777777;
}

.legend {
	text-align: left;
	line-height: 20px;
	color: #555;
}

.legend i {
	width: 40px;
	height: 20px;
	float: left;
	margin-left: 8px;
	opacity: 0.7;
}

.slider.slider-horizontal {
	width: 400px;
}

.tooltip-inner {
	margin-top: 20px;
	font-family: "微软雅黑";
	font-size: 14px;
	background: #FFFFFF;
	color: #000000;
}

.axis path,
.axis line {
	fill: none;
	stroke: #000;
	shape-rendering: crispedges;
}

.axis text {
	fill: #000;
	font-size: 14px;
}

#time-panel {
	display: none;
	background: rgba(255, 255, 255, .8);
	border: 1px solid #acb2bb;
	padding: 13px;
	margin-top: 5px;
	overflow: hidden;
	position: absolute;
	z-index: 999;
	right: 20px;
	top: 100px;
}

.time-panel-progress-container {
	float: left;
	width: 430px;
}

.time-panel-progress-bar {
	margin: 0 auto;
	position: absolute;
	left: 20px;
}

#timeline {
	/*display: none;*/
}

.time-panel-progress-tick {
	margin: 0 auto;
	position: absolute;
	top: 35px;
	left: 20px;
}

#tick {
	height: 30px;
	width: 500px;
	left: -25px;
	position: absolute;
}

.time-panel-btn {
	background: url(../../static/lbs/time-btn-pause.png) no-repeat 0 0;
	width: 33px;
	height: 33px;
	float: right;
	cursor: pointer;
}

.play {
	background: url(../../static/lbs/time-btn-play.png) no-repeat 0 0;
}