@CHARSET "UTF-8";

body {
	overflow-y:scroll;
	padding-top: 51px;
	margin-left: 0px;
	margin-top: 0px;
	font-family: "Source Sans Pro","Helvetica Neue",Arial,"Hiragino Sans GB","Microsoft YaHei","WenQuanYi Micro Hei",sans-serif;
	font-weight: 400;
    font-size: 16px;
}

/*-----nav-----*/
.navbar {
	background-color: #2C3E50;
	color: #ECF0F1;
	font-size: 16px;
}
.navbar-inverse .navbar-nav>.active>a, .navbar-inverse .navbar-nav>.active>a:focus, .navbar-inverse .navbar-nav>.active>a:hover {
	background-color: #34495E;
	color: #ECF0F1;
}
.logo {
	margin-left: 8px;
    font-size: 12px;
    background: url(../../static/index/logo.png);
    background-position: center center;
    background-repeat: no-repeat;
    background-size: 7em auto;
    float: left;
    opacity: .9;
    -webkit-transition: opacity .2s ease 0s;
    transition: opacity .2s ease 0s;
    width: 12.5em;
}
.logo:hover {
	margin-left: 8px;
    font-size: 12px;
    background: url(../../static/index/logo-sel.png);
    background-position: center center;
    background-repeat: no-repeat;
    background-size: 7em auto;
    float: left;
    opacity: .9;
    -webkit-transition: opacity .2s ease 0s;
    transition: opacity .2s ease 0s;
    width: 12.5em;
}
.navbar-collapse {
	float: right;
}

/*-----layout-----*/
.col-xs-1, .col-xs-10 {
	padding-right:0px;
}

.col-xs-1, .col-xs-10{
	padding-left:0px;
}

.listName{
	background: rgba(153, 153, 153, .8);
}

/*-- left_panel --*/
.panel-group {
	margin-top: 15px;
}
.work_panel {
	background-color: #ECF0F1;
	height:100%
}
.panel-title a {
	font-size:16px;
	color:#0da0fd;
	border: 1.6px solid #79bae5;
	border-radius: 5px;
	padding: 5px;
	text-align: center;
	padding-right: 5%;
	font-weight: 400;
}
.panel_menu {
	margin-bottom: 20px;
}
.a.list-group-item {
	font-size: 14px;
}
.list-group-item {
    padding: 1px 18px;
}
/*-- center_panel --*/
.center_panel {
	background-image: url(../../static/img/grid.gif);
	background-repeat: repeat;
}
#workspace {
	width: 100%;
	/*background-image: url(../static/img/grid.gif);*/
	height:600px;
	background-repeat: repeat;
}
svg {
	height: 500px;
	width : 100%;
	float: left;
}
.btn-group-vertical{
	width: 100%
}
.btn{
	height: 40px;
}
/*-- data_analyse --*/
.data_analyse {
	margin-left: auto; 
	margin-right: auto; 
	background: rgba(0,0,0,0.1);
	padding: 10px;
	min-height: 50px;
}
.placeholder {
	width: 0px;
	float: left;
	z-index: 2147483647;
}
.tool_filter {
	/*background:rgba(0,0,0,0.3);*/
	background: none;
}
.btn-h3 {
	height: 27px;
	font-size: 14px;
	font-weight: 400;
	color: #0da0fd;
	border: 1px solid #79bae5;
	border-radius: 5px;
	padding: 3px;
	text-align: center;
	background: rgba(255,255,255,0.2);
}
.btn-h3:hover, .btn-h3:active ,.btn-h3:checked, .btn-h3:focus {
	background: #3498DB;
	border: 1px solid #3498DB;
}
/*filter*/
.filter_condition {
	width: 110px;
}
.filter_panel_title {
	margin: 20px;
	margin-top: 0px;
}
.filter_panel_heading {
	margin-bottom: 20px;
}
/*-- chart_select --*/
.chart_select{
	padding-right: 20px;
	padding-top: 20px;
	float: right;
	width: 100%;
}
.list-chart {
	padding:0;
	margin: 5px 0px;
}
.menu img{
	margin: 5px 0 5px 0;
	height: 25%;
	width: 25%;
}
.menu {
	margin: 2px 15px;
	padding: 5px 0px;
	font-size: 14px;
	text-align: center;
	list-style-type: none;
}
.menu:hover {
	color: #3498DB;
}
.menu_content {
	float: right;
	width: 165px;
	margin: 5px;
	margin-top: 5px;
	border:2px solid #EEEEEE;
	border-radius: 15px;
	background-color: #FDFDFD;
	-webkit-box-shadow:0 0 5px #F9F9F9;
	-moz-box-shadow:0 0 5px #F9F9F9;
	box-shadow:0 0 5px #F9F9F9;
	z-index:2147483647;
}
.fig {
	text-align: center;
	float: left;
	width: 80px;
}
.fig img {
	width: 80%;
	height: 80%;
	margin: -20px;
	margin-top: 0px;
}
.fig p {
	font-size: 8px;
	margin-bottom: 10px;
	margin-top: 18px;
}


/* table style */
.changeType {
	border: 1px solid #bbb;
	color: #bbb;
	background: #eee;
	border-radius: 2px;
	padding: 2px;
	font-size: 9px;
	float: right;
	line-height: 9px;
	margin: 3px 3px 0 0;
}

.changeType:hover {
	border: 1px solid #777;
	color: #777;
	cursor: pointer;
}

.changeType.pressed {
	background-color: #999;
}

.changeTypeMenu {
	position: absolute;
	border: 1px solid #ccc;
	margin-top: 18px;
	box-shadow: 0 1px 3px -1px #323232;
	background: white;
	padding: 0;
	font-size: 13px;
	display: none;
	z-index: 10;
}

.changeTypeMenu li {
	text-align: left;
	list-style: none;
	padding: 2px 20px;
	cursor: pointer;
	margin-bottom: 0;
}

.changeTypeMenu li.active:before {
	font-size: 12px;
	content: "\2714";
	margin-left: -15px;
	margin-right: 3px;
}

.changeTypeMenu li:hover {
	background: #eee;
}
